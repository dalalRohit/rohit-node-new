<div class="container" id="main">
  <!-- flash for login -->
  {{#if msg}}
  <div class="alert alert-success">
      {{msg}}
  </div>
  {{/if}}

<h4>  Dashboard {{user.loginid}} </h4>
<h4>Year: {{roll.year}} Dept: {{roll.dept}} Div: {{roll.div}} RollNo: {{roll.no}}</h4>

<h5>Showing teachers of dept code {{roll.dept}}</h5>
<div class="alert alert-info">
  <b>Note: </b> <h5>Click on the teacher's name to give feedback!</h5>
</div>

{{#if roll}}
<h2>Roll details:</h2>
  {{#each roll}}
    {{this}}
  {{/each}}
{{/if}}

{{#if token}}
  {{token}}
{{/if}}
<!-- SHOWING TEACHERS ACCORDING TO DEPARTMENT -->
{{#if teachers}}
  <ul class="teachers">
    {{#each teachers}}
      <li> <a href="/users/give-feedback">{{this}}</a> </li>
    {{/each}}
  </ul>
{{/if}}
</div>
